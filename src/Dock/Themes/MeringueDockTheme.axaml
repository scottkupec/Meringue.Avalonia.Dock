  <StyleInclude Source="/Themes/DockHostRoot.axaml" />
  <StyleInclude Source="/Themes/DockLayoutRoot.axaml" />
  <StyleInclude Source="/Themes/DockSplitPanel.axaml" />
  <StyleInclude Source="/Themes/DockTabPanel.axaml" />
  <StyleInclude Source="/Themes/DockTool.axaml" />
  <StyleInclude Source="/Themes/DockToolStub.axaml" />

</Styles>
