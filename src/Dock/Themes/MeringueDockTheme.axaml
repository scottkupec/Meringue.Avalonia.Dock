  x:Class="Meringue.Avalonia.Dock.Themes.MeringueDockTheme">

  <Styles.Resources>
    <ResourceDictionary>
      <converters:PinIconConverter x:Key="PinIconConverter" />
      <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
    </ResourceDictionary>
